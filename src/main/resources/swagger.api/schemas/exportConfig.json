{
  "ExportConfig": {
    "description": "Export Configuration",
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "scheduleFrequency": {
        "type": "integer",
        "description": "Number of time periods"
      },
      "schedulePeriod": {
        "type": "string",
        "description": "Time period for repeating job",
        "enum": [
          "WEEK",
          "DAY",
          "HOUR",
          "NONE"
        ]
      },
      "scheduleTime": {
        "type": "string",
        "description": "Time to run the job"
      },
      "weekDays": {
        "type": "array",
        "description": "Day of week to run the job",
        "items": {
          "type": "string",
          "description": "Day of week",
          "enum": [
            "MONDAY",
            "TUESDAY",
            "WEDNESDAY",
            "THURSDAY",
            "FRIDAY",
            "SATURDAY",
            "SUNDAY"
          ]
        }
      },
      "ftpUrl": {
        "type": "string",
        "description": "Url to ftp server"
      },
      "daysOutstanding": {
        "type": "integer",
        "description": "Days outstanding"
      },
      "patronGroups": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "additionalProperties": false
  }
}
